{
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "globals": {
    "shallow": true,
    "mount": true,
    "render": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "plugins": ["import", "babel", "jest", "jsx-a11y", "react"],
  "rules": {
    "array-bracket-spacing": [2, "always"],
    "arrow-parens": 2,
    "arrow-spacing": [2, { "after": true, "before": true }],
    "babel/semi": [2, "always"],
    "brace-style": [2, "stroustrup", { "allowSingleLine": true }],
    "callback-return": 2,
    "camelcase": 2,
    "comma-dangle": [
      2,
      {
        "arrays": "always-multiline",
        "exports": "always-multiline",
        "functions": "always-multiline",
        "imports": "always-multiline",
        "objects": "always-multiline"
      }
    ],
    "comma-spacing": [2, { "after": true, "before": false }],
    "computed-property-spacing": [2, "never"],
    "consistent-return": 0,
    "constructor-super": 2,
    "curly": 2,
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": 1,
    "func-names": 0,
    "generator-star-spacing": [2, { "after": false, "before": true }],
    "guard-for-in": 1,
    "handle-callback-err": 2,
    "indent": [2, 2, { "SwitchCase": 1 }],
    "init-declarations": [2, "always"],
    "key-spacing": [2, { "afterColon": true, "beforeColon": false }],
    "keyword-spacing": 2,
    "max-depth": [1, { "max": 10 }],
    "max-len": [
      2,
      { "code": 120, "ignoreComments": true, "ignoreUrls": true, "tabWidth": 1 }
    ],
    "max-nested-callbacks": [1, { "max": 4 }],
    "max-params": [1, { "max": 3 }],
    "new-cap": 0,
    "new-parens": 2,
    "newline-after-var": 2,
    "no-alert": 1,
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-class-assign": 2,
    "no-cond-assign": [2, "except-parens"],
    "no-console": 1,
    "no-const-assign": 2,
    "no-constant-condition": 1,
    "no-debugger": 1,
    "no-dupe-class-members": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-duplicate-imports": 2,
    "no-empty": 2,
    "no-empty-pattern": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": 0,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-implied-eval": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": [2, { "allowLoop": true, "allowSwitch": true }],
    "no-lone-blocks": 2,
    "no-loop-func": 1,
    "no-mixed-requires": 0,
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": [2, { "max": 2 }],
    "no-native-reassign": 2,
    "no-nested-ternary": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-symbol": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-path-concat": 1,
    "no-proto": 2,
    "no-return-await": 2,
    "no-self-assign": 2,
    "no-sequences": 2,
    "no-shadow": 1,
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-underscore-dangle": 0,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "no-var": 2,
    "no-with": 2,
    "object-curly-spacing": [2, "always"],
    "object-shorthand": [2, "always"],
    "one-var": [2, "never"],
    "operator-linebreak": [2, "after"],
    "padded-blocks": [2, "never"],
    "prefer-const": 2,
    "prefer-spread": 2,
    "quotes": [2, "single"],
    "radix": 2,
    "react/jsx-curly-spacing": [1, "never"],
    "react/jsx-equals-spacing": 1,
    "react/jsx-no-bind": [1, { "allowArrowFunctions": true }],
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-undef": 2,
    "react/jsx-pascal-case": 2,
    "react/jsx-space-before-closing": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 2,
    "react/no-deprecated": 1,
    "react/no-did-update-set-state": 1,
    "react/no-string-refs": 1,
    "react/no-unknown-property": 1,
    "react/react-in-jsx-scope": 2,
    "react/self-closing-comp": 1,
    "require-await": 2,
    "semi-spacing": [2, { "after": true, "before": false }],
    "space-before-blocks": 2,
    "space-before-function-paren": 2,
    "space-in-parens": [2, "never"],
    "space-infix-ops": [2, { "int32Hint": false }],
    "space-unary-ops": [2, { "nonwords": false, "words": true }],
    "strict": [2, "never"],
    "use-isnan": 2
  }
}
